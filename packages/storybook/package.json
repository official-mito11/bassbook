{
  "name": "@bassbook/storybook",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap --clean --out-dir dist",
    "generate": "bun run scripts/generate-stories.ts",
    "dev": "bun run generate && storybook dev -p 6006",
    "storybook": "bun run generate && storybook dev -p 6006",
    "storybook:build": "bun run generate && storybook build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@types/bun": "latest",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^19.2.2",
    "react-dom": "^19.2.2",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "typescript": "^5"
  },
  "dependencies": {
    "@bassbook/core": "workspace:*",
    "@bassbook/react": "workspace:*",
    "storybook": "^8.0.0"
  }
}